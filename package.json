{
  "name": "fragmentarium-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:server": "python3 scripts/start_server.py",
    "prebuild": "node scripts/bump_build_version.mjs",
